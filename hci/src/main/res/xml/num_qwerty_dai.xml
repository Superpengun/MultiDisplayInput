<?xml version='1.0' encoding='utf-8'?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/com.sinovoice.hciimevehicleintl"
    android:horizontalGap="@dimen/keyboard_key_horizontalGap"
          android:keyHeight="@dimen/keyboard_key_height"
          android:keyWidth="@dimen/keyboard_key_width"
          android:verticalGap="@dimen/keyboard_key_verticalGap">

    <Row>
        <Key
            android:keyEdgeFlags="left"
            android:keyLabel="1"
            android:popupCharacters="1๑"/>
        <Key
            android:keyLabel="2"
            android:popupCharacters="2๒"/>
        <Key
            android:keyLabel="3"
            android:popupCharacters="3๓"/>
        <Key
            android:keyLabel="4"
            android:popupCharacters="4๔"/>
        <Key
            android:keyLabel="5"
            android:popupCharacters="5๕"/>
        <Key
            android:keyLabel="6"
            android:popupCharacters="6๖"/>
        <Key
            android:keyLabel="7"
            android:popupCharacters="7๗"/>
        <Key
            android:keyLabel="8"
            android:popupCharacters="8๘"/>
        <Key
            android:keyLabel="9"
            android:popupCharacters="9๙"/>
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="0"
            android:popupCharacters="0๐"/>
    </Row>
    <Row>
        <Key
            android:keyLabel="-"/>
        <Key
            android:keyLabel="/"/>
        <Key
            android:keyLabel=":"/>
        <Key
            android:keyLabel=";"/>
        <Key
            android:keyLabel="("/>
        <Key
            android:keyLabel=")"/>
        <Key
            android:keyLabel="฿"/>
        <Key
            android:keyLabel="&amp;"/>
        <Key
            android:keyLabel="\@"/>
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel='"'/>
    </Row>


    <Row>

        <Key
            android:backgroundType="functional"
            android:codes="-71"
            android:keyLabel="#+="
            android:keyWidth="@dimen/keyboard_function_key_width"/>
        <Key
            android:keyWidth="14%p"
            android:keyLabel="."/>
        <Key
            android:keyWidth="14%p"
            android:keyLabel=","/>
        <Key
            android:keyWidth="14%p"
            android:keyLabel="\?"/>
        <Key
            android:keyWidth="14%p"
            android:keyLabel="!"/>
        <Key
            android:keyWidth="14%p"
            android:keyLabel="'"/>
        <Key
            android:backgroundType="functional"
            android:codes="-4"
            android:isRepeatable="true"
            android:keyIcon="@drawable/icon_delete"
            android:keyWidth="@dimen/keyboard_function_key_width"/>
    </Row>

    <Row>

        <Key
            android:backgroundType="functional"
            android:codes="-21"
            android:keyEdgeFlags="left"
            android:keyWidth="@dimen/keyboard_function_key_width"
            android:keyLabel="กขค"
            />


        <Key
            android:codes="-3"
            android:keyIcon="@drawable/icon_space"
            android:keyWidth="55%p"/>


        <Key
            android:backgroundType="functional"
            android:codes="-111"
            android:keyWidth="@dimen/keyboard_function_key_width"
            android:keyIcon="@drawable/icon_lang"/>
        <Key
            android:backgroundType="functional"
            android:codes="-2"
            android:keyWidth="@dimen/keyboard_function_key_width"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/icon_enter"/>
    </Row>



</Keyboard>